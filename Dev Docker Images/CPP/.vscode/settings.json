{
    "files.associations": {
        "sstream": "cpp",
        "map": "cpp",
        "*.tcc": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "functional": "cpp",
        "future": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "ostream": "cpp",
        "iostream": "cpp"
    },
    "code-runner.executorMap": {
        "c": "cd $dir && clang $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && clang++ $fileName -o $fileNameWithoutExt && cat a.in | $dir$fileNameWithoutExt",
    }
}